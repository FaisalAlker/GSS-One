{
  "name": "gss-one",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "build": {
    "appId": "com.alker.gss.one",
    "productName": "GSS One",
    "icon": "icon.ico",
    "files": [
      "dist/**/*",
      "package.json",
      "ipc/**/*",
      "main.js",
      "preload.js",
      "config.js",
      "icon.ico",
      "!**/node_modules/.cache/**/*",
      "!**/node_modules/**/*.md",
      "!**/node_modules/**/test/**/*",
      "!**/node_modules/**/tests/**/*"
    ],
    "directories": {
      "output": "release",
      "buildResources": "."
    },
    "asar": true,
    "win": {
      "icon": "icon.ico",
      "target": [
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        }
      ]
    }
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development electron .",
    "build": "electron-builder"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.18.2",
  "devDependencies": {
    "electron": "^38.2.2",
    "electron-builder": "^26.0.12",
    "cross-env": "^10.1.0"
  },
  "dependencies": {
    "electron-store": "^11.0.2"
  }
}